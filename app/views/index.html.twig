{% extends "base.html.twig" %}
{% block contents %}
<h2>List of projects</h2>
<ul>
  {% for project in projects %}
  <li><a href="/projects/{{project.id}}">{{ project.name }}</a></li>
  {% endfor %}
</ul>
<h2>List of items</h2>
<ul>
  {% for item in items %}
  <li><a href="/items/{{item.id}}">{{ item.title }}</a></li>
  {% endfor %}
</ul>

{% endblock contents %}
